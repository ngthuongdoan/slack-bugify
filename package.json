{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec 'ts-node --ignore node_modules --project tsconfig.json src/server.ts'",
    "build": "tsc --project tsconfig.json && tsc-alias",
    "start": "esno dist/server.js",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-flash": "0.0.2",
    "express-session": "1.17.1",
    "express-validator": "6.9.2",
    "googleapis": "105",
    "helmet": "^6.1.5",
    "http-status": "^1.6.2",
    "joi": "^17.9.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "tsc-alias": "^1.8.5",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@google-cloud/local-auth": "2.1.0",
    "@types/async": "3.2.5",
    "@types/bson": "4.0.5",
    "@types/chai": "4.2.14",
    "@types/compression": "1.7.0",
    "@types/concurrently": "5.2.1",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.11",
    "@types/express-flash": "0.0.2",
    "@types/express-session": "1.17.3",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16",
    "@types/request": "2.48.5",
    "@types/shelljs": "0.8.8",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "concurrently": "6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "7.19.0",
    "esno": "^0.16.3",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "shelljs": "0.8.5",
    "supertest": "6.1.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
